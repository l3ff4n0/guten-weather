{
	"apiVersion": 3,
	"name": "cloud-weather/cloud-weather",
	"version": "0.3.2",
	"title": "Cloud weather",
	"category": "widgets",
	"icon": "cloud",
	"keywords": ["weather", "forecast", "temperature"],
	"description": "This is a gutenberg weather block to display weather and forecast",
	"supports": {
		"html": false
	},
	"attributes":{
		"city": {
			"type": "string",
			"default": "city name"
		},
		"weatherType": {
			"type": "string",
			"default": "current"
		},
		"numberDays": {
			"type": "string",
			"default": "1"
		},
		"languageData": {
			"type": "string",
			"default": "select your language"
		},
		"layoutModel": {
			"type": "string",
			"default": "select your layout"
		},
		"borderRadius": {
			"type": "number",
			"default": 0
		},
		"WidgetBgColor": {
			"type": "string",
			"default": "#fff"
		},
		"WidgetColor": {
			"type": "string",
			"default": "#000"
		}
	},
	"textdomain": "cloud-weather",
	"script": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}